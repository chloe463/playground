    "build": "next build",
    "test": "TZ=Asia/Tokyo jest --config=jest.config.js",
    "download-schema:local": "node scripts/download.mjs",
    "codegen": "graphql-codegen --config codegen.js",
    "codegen:clean": "./scripts/clean-types.sh",
    "codegen:watch": "yarn codegen --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "serve": "NEXT_PUBLIC_API_MOCK=true next dev",
    "cypress:open": "cd e2e && yarn cypress:open",
    "cypress:run": "cd e2e && yarn cypress:run",
    "cypress:ci": "start-server-and-test serve http-get://localhost:3000 cypress:run"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
